<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.graduate.mapper.RegisterMapper">
    <!--查找-->
    <select id="find" resultType="com.graduate.pojo.Users">
        select *
        from users where username=#{username};
    </select>
    <!--保存-->
    <insert id="save">
        insert into
        users(
            username, password,  name, phone
        )
        values(
               #{users.userName},
               #{users.passWord},
               #{users.name},
               #{users.phone}
        );
    </insert>

</mapper>